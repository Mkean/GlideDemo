<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.glidedemo">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.BasisActivity" />
        <activity android:name=".activity.DownloadActivity" />
        <activity android:name=".activity.TargetActivity" />
        <activity android:name=".activity.ListenerAndAnimationActivity" />
        <activity android:name=".activity.PriorityActivity" />
        <activity android:name=".activity.CustomActivity" />

        <meta-data
            android:name="com.example.glidedemo.custom.MyGlideModule"
            android:value="GlideModule" />

        <activity android:name=".activity.ProgressActivity" />
        <activity android:name=".activity.PhotoWallActivity" />
        <activity android:name=".activity.GlideUrlActivity"></activity>
    </application>

</manifest>